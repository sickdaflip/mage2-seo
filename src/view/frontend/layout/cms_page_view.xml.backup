<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="head.additional">
            <block class="FlipDev\Seo\Block\CmsCanonical" name="flipdevseo.cmscanonical" template="FlipDev_Seo::cms/canonical.phtml" ifconfig="flipdev_seo/settings/enablecmscanonical" />
        </referenceBlock>
        <referenceBlock name="footer">
            <block class="FlipDev\Seo\Block\Template" name="flipdevseo.content.grouping" template="FlipDev_Seo::google/content-grouping.phtml" ifconfig="flipdev_seo/google_content_grouping/cms">
                <action method="setType">
                    <argument name="name" xsi:type="string">cms</argument>
                </action>
             </block>
        </referenceBlock>
    </body>
</page>
